            "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
        }
    },
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@babel/cli": "^7.15.7",
        "@babel/core": "^7.15.8",
        "@babel/preset-env": "^7.15.8",
        "@babel/preset-react": "^7.14.5",
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "axios": "^0.22.0",
        "dotenv": "^10.0.0",
